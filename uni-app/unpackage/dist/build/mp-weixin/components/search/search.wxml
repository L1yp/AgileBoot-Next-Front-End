<view><view hidden="{{!(historyList.length)}}" class="history"><view class="title"><text class="txt">搜索历史</text></view><view class="del-icon"><view data-event-opts="{{[['tap',[['del',['$event']]]]]}}" class="iconfont _i" bindtap="__e"></view></view></view><view class="list"><block wx:for="{{historyList}}" wx:for-item="v" wx:for-index="k" wx:key="k"><text data-event-opts="{{[['tap',[['historyClick',['$0',k],[[['historyList','',k]]]]]]]}}" class="msg-box" bindtap="__e">{{v.value}}</text></block></view><view class="history"><view class="title"><text class="txt">搜索发现</text></view><block wx:if="{{isShow}}"><view class="del-icon"><view data-event-opts="{{[['tap',[['see',['$event']]]]]}}" class="iconfont _i" bindtap="__e"></view></view></block><block wx:else><view class="del-icon"><view data-event-opts="{{[['tap',[['see',['$event']]]]]}}" class="iconfont _i" bindtap="__e"></view></view></block></view><block wx:if="{{isShow}}"><view class="find"><block wx:for="{{findList}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view class="flex">{{v.value}}</view></block></view></block><block wx:else><view class="find-hide"><text class="hide">当前搜索发现已隐藏</text></view></block></view>