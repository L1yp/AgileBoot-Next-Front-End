page{background-color:#f4f8fe;height:100%;overflow:hidden}.flex-warp{display:-webkit-box;display:-webkit-flex;display:flex;-webkit-box-orient:vertical;-webkit-box-direction:normal;-webkit-flex-direction:column;flex-direction:column;height:100%;overflow:hidden}.flex-warp .order{display:-webkit-box;display:-webkit-flex;display:flex;height:100rpx;background-color:#fff;font-size:32rpx;position:relative}.flex-warp .order::after{content:"";background:#dcdcdc;position:absolute;width:100%;height:1px;left:0;bottom:0;-webkit-transform:scaleY(.5);transform:scaleY(.5)}.flex-warp .order .flex{-webkit-box-flex:1;-webkit-flex:1;flex:1;display:-webkit-box;display:-webkit-flex;display:flex;position:relative}.flex-warp .order .flex .nav{margin:auto}.flex-warp .order .flex .nav-border{position:absolute;width:40rpx;height:5rpx;background:#198cff;top:90rpx;left:50%;-webkit-transform:translateX(-50%);transform:translateX(-50%)}.flex-warp .order .flex .active{color:#198cff}.flex-warp .swiper{-webkit-box-flex:1;-webkit-flex:1;flex:1}.flex-warp .overflow{overflow:auto;height:100%}