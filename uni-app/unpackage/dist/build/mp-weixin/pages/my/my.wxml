<view class="content"><view class="user"><view class="user-content"><view class="user-content-warp"><view class="user-photo"><image class="photo" src="{{userInfo.photo}}"></image></view><view class="user-info"><view data-event-opts="{{[['tap',[['goToMyInfo',['$event']]]]]}}" class="user-info-nane" bindtap="__e"><text>{{userInfo.name}}</text><view class="iconfont name-arrow _i"></view></view><view class="user-info-nane authentication"><view class="iconfont _i"></view><text class="txt">{{userInfo.isRenz}}</text></view></view></view></view></view><view class="tabs"><block wx:for="{{tabsList}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view data-event-opts="{{[['tap',[['handleTabs',['$0',k],[[['tabsList','',k]]]]]]]}}" class="tabs-warp" bindtap="__e"><view class="tabs-warp-item"><text class="value">{{v.value}}</text><text class="title">{{v.title}}</text></view></view></block></view><view class="table-list"><block wx:for="{{tableList}}" wx:for-item="v" wx:for-index="k" wx:key="k"><view data-event-opts="{{[['tap',[['handleTable',['$0',k],[[['tableList','',k]]]]]]]}}" class="table-list-warp" bindtap="__e"><view class="icon" style="{{('color:'+v.color)}}"><view class="iconfont _i">{{v.icon}}</view></view><view class="title"><text>{{v.title}}</text></view><view class="arrow"><view class="iconfont _i"></view></view></view></block></view></view>